  "components": {
    "schemas": {
      "ChangePassordDto": {
        "required": [
          "email",
          "newPassword"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "newPassword": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "CreatSubscriptionDTo": {
        "required": [
          "amount",
          "currency",
          "name",
          "timeOfSubscription"
        ],
        "type": "object",
        "properties": {
          "name": {
            "maxLength": 50,
            "minLength": 1,
            "type": "string"
          },
          "videoQuality": {
            "$ref": "#/components/schemas/VideoQuality"
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "acousticCharacteristics": {
            "type": "string",
            "nullable": true
          },
          "numberOfLetters": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "amount": {
            "type": "number",
            "format": "double"
          },
          "currency": {
            "$ref": "#/components/schemas/CurrencyType"
          },
          "timeOfSubscription": {
            "$ref": "#/components/schemas/SubscriptionPeriod"
          },
          "isActive": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "CurrencyType": {
        "enum": [
          "USD",
          "KSA"
        ],
        "type": "string"
      },
      "EditUserDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginDto": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "password": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "RoleType": {
        "enum": [
          "Admin",
          "Writer",
          "Financial_Accounts"
        ],
        "type": "string"
      },
      "SubscriptionPeriod": {
        "enum": [
          "OneMonth",
          "ThreeMonths",
          "SixMonths",
          "OneYear"
        ],
        "type": "string"
      },
      "VideoQuality": {
        "enum": [
          "SD",
          "HD",
          "FullHD",
          "UltraHD"
        ],
        "type": "string"
      }
